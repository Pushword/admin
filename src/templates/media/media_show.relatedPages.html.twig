{% if related_pages %}
<ul>
    {% for related_page in related_pages %}
        {% block item3 %}
            <li>
                <a href="{{ path('admin_page_edit', {id: related_page.id}) }}">
                    {{ pw(related_page).h1|raw }}
                </a>
            </li>
        {% endblock %}
    {% endfor %}
</ul>
{% endif %}
