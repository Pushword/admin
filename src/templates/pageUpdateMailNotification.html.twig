
<ul>
{% for page in pages %}
    <li>
        <code>{{ page.slug }}</code><br>
        <a href="{{ url('pushword_page', {slug: page.slug}) }}">{{ pw(page).h1 }}</a><br>
        Page mise à jour le {{ page.updatedAt|date('d/m/Y à H:i') }} <br>
        <a href="{{ url('admin_page_edit', {id: page.id}) }}">Editer</a>
    </li>
{% endfor %}
</ul>
